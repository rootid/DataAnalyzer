<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label>Lattitude</g:Label>
			<g:TextBox ui:field='latTextBox'></g:TextBox>
		</g:HorizontalPanel>

		<g:HorizontalPanel>
			<g:Label>Longitude</g:Label>
			<g:TextBox ui:field='longTextBox'></g:TextBox>
		</g:HorizontalPanel>
	<g:HorizontalPanel>
		<g:Button ui:field='submit'> Submit </g:Button>
		<g:Button ui:field='cancel'> Cancel </g:Button>
	</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>