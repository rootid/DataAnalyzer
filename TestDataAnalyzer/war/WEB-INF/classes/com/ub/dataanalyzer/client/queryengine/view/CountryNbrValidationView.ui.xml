<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label>Country-X</g:Label>
			<g:ListBox ui:field="countryXListBox" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label>Country-Y</g:Label>
			<g:ListBox ui:field="countryYListBox" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Button ui:field='submit'> Submit </g:Button>
			<g:Button ui:field='cancel'> Cancel </g:Button>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>